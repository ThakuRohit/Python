    "def isalnum(string):\n",
    "    if string == \"\":\n",
    "        return False\n",
    "    for s in string:\n",
    "        if not ('a' <= s <= 'z' or 'A' <= s <= 'Z' or '0' <= s <= '9'):\n",
    "            return False\n",
    "    return True"
