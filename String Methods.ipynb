    "    string_list_1 = []\n",
    "    string_list = list(string)\n",
    "    for i in string:\n",
    "        if i >= 'A' and i <= 'Z':\n",
    "            new_i = chr(ord(i) + 32)\n",
    "            string_list_1.append(new_i)\n",
    "        if i >= 'a' and i <= 'z':\n",
    "            new_i = chr(ord(i)-32)\n",
    "            string_list_1.append(new_i)\n",
    "            \n",
    "    return ''.join(string_list_1)"
