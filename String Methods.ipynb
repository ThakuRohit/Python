    "def replacestr(string,old_value,new_value,count):\n",
    "    new_string = string + \" \"\n",
    "    list = []\n",
    "    startp = 0 \n",
    "    final_string = \"\"\n",
    "    for s in range(len(new_string)):\n",
    "        if new_string[s] == \" \":\n",
    "            list.append(new_string[startp:s])\n",
    "            startp = s+1\n",
    "    c = 0\n",
    "    new_list = []\n",
    "    for i in list :\n",
    "        if i == old_value and c < count:\n",
    "            i = new_value\n",
    "            c +=1\n",
    "        new_list.append(i)\n",
    "    for i in new_list :\n",
    "        final_string = final_string + i + \" \"\n",
    "    return final_string[:len(final_string)-1]"
   ]
