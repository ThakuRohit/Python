    "def stripstr(string,value):\n",
    "    start = 0\n",
    "    end = len(string)\n",
    "    while start<end and string[start] == value :\n",
    "            start+=1\n",
    "    while end > start and string[end-1] == value:\n",
    "            end -=1\n",
    "    return string[start:end]"
   ]
