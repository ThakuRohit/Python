    "def titlesen(string):\n",
    "    string_list = string.strip().split()\n",
    "    new_string_list = []\n",
    "    for i in (string_list):\n",
    "        j = i[0]\n",
    "        if j >= 'A' and j <= 'Z':\n",
    "            new_i = chr(ord(j) + 32)\n",
    "            j = new_i\n",
    "        if j >= 'a' and j <= 'z':\n",
    "            new_i = chr(ord(j)-32)\n",
    "            j = new_i\n",
    "        i = j + i[1:]\n",
    "        new_string_list.append(i)\n",
    "    return new_string_list"
   ]
